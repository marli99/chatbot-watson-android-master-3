<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9691FF"
    tools:context=".Dashboard">

    <TextView
        android:layout_width="231dp"
        android:layout_height="70dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="10dp"
        android:fontFamily="cursive"
        android:text="Choose Topic"
        android:textColor="#ffffff"
        android:textSize="15pt">

    </TextView>

    <Button
        android:id="@+id/depressionID"
        android:layout_width="420dp"
        android:layout_height="140dp"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/depressed_wallpaper"
        android:padding="5dp"
        android:text="Depression"
        android:textColor="#ffffff"
        android:textSize="9pt"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"></Button>

    <Button
        android:id="@+id/anxietyID"
        android:layout_width="420dp"
        android:textColor="#ffffff"
        android:textSize="9pt"
        android:textStyle="bold"
        android:layout_height="140dp"
        android:layout_marginBottom="20dp"
        android:text="Anxiety"
        android:background="@drawable/anxiety"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="240dp"></Button>

    <Button
        android:id="@+id/stressID"
        android:layout_width="420dp"
        android:layout_height="140dp"
        android:textColor="#ffffff"
        android:textSize="9pt"
        android:textStyle="bold"
        android:layout_marginBottom="20dp"
        android:text="Stress"
        android:background="@drawable/pathtwenty"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="390dp"></Button>

    <Button
        android:id="@+id/angerID"
        android:layout_width="420dp"
        android:textColor="#ffffff"
        android:textSize="9pt"
        android:textStyle="bold"
        android:layout_height="140dp"
        android:layout_marginBottom="20dp"
        android:text="Anger"
        tools:ignore="MissingConstraints"
        android:background="@drawable/anger"
        android:layout_marginTop="540dp"></Button>

</RelativeLayout>